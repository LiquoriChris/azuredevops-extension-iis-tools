{
	"manifestVersion": 1,
	"id": "iis-tools",
	"name": "IIS Tools",
	"description": "A collection of tools to manage and interact with IIS",
	"version": "1.2.1",
	"publisher": "pwshliquori",
	"public":  true,
	"baseUri": "https://github.com/USF-HII",
	"scopes": [],
	"tags": [
		"git",
		"tags",
		"release"
	],
	"links": {
		"home": {
			"uri": "https://github.com/USF-HII/azuredevops-extension-iis-tools"
		},
		"learn": {
			"uri": "https://github.com/USF-HII/azuredevops-extension-iis-tools"
		},
		"support": {
			"uri": "https://github.com/USF-HII/azuredevops-extension-iis-tools/issues"
		},
		"repository": {
			"uri": "https://github.com/USF-HII/azuredevops-extension-iis-tools"
		}
	},
	"repository": {
		"type": "git",
		"uri": "https://github.com/USF-HII/azuredevops-extension-iis-tools"
	},
	"branding": {
		"color": "rgb(0, 114, 186)",
		"theme": "dark"
	},
	"targets": [
		{
			"id": "Microsoft.VisualStudio.Services"
		}
	],
	"categories": [
		"Azure Pipelines"
	],
	"icons": {
		"default": "logo.png"
	},
	"files": [
		{ "path": "Tasks/DisableIISApplication" },
		{ "path": "Tasks/EnableIISApplication" },
		{ "path": "Tasks/StopIISApplicationPool" },
		{ "path": "Tasks/StartIISApplicationPool" }
	],
	"content": {
		"details": {
			"path": "overview.md"
		},
		"license": {
			"path": "license.md"
		}
	},
	"contributions": [
		{
			"id": "disable-iis-application",
			"type": "ms.vss-distributed-task.task",
			"targets": [
				"ms.vss-distributed-task.tasks"
			],
			"properties": {
				"name": "Tasks/DisableIISApplication"
			}
		},
		{
			"id": "enable-iis-application",
			"type": "ms.vss-distributed-task.task",
			"targets": [
				"ms.vss-distributed-task.tasks"
			],
			"properties": {
				"name": "Tasks/EnableIISApplication"
			}
		},
		{
			"id": "stop-iis-apppool",
			"type": "ms.vss-distributed-task.task",
			"targets": [
				"ms.vss-distributed-task.tasks"
			],
			"properties": {
				"name": "Tasks/StopIISApplicationPool"
			}
		},
		{
			"id": "start-iis-apppool",
			"type": "ms.vss-distributed-task.task",
			"targets": [
				"ms.vss-distributed-task.tasks"
			],
			"properties": {
				"name": "Tasks/StartIISApplicationPool"
			}
		}
	]
}